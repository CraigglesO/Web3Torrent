{
  "name": "dapps-react-ui",
  "version": "0.0.684",
  "description": "React ui reusable components for dapps",
  "main": "./build/index.js",
  "scripts": {
    "install": "napa alexvandesande/blockies",
    "start": "webpack-dev-server -d --history-api-fallback --hot --inline --progress --colors --port 3020",
    "startPerf": "NODE_ENV=perf-debug npm start",
    "startTest": "NODE_ENV=tests npm start",
    "webpack": "webpack",
    "commit": "commit-wizard",
    "coveralls": "npm run testCoverage && coveralls < coverage/lcov.info",
    "lint": "eslint --ignore-path .gitignore .",
    "test": "npm run testOnce -- -w",
    "testCoverage": "istanbul cover _mocha -- 'src/**/*.spec.js'",
    "testOnce": "mocha 'src/**/*.spec.js'",
    "prebuild": "npm run clean",
    "build": "NODE_ENV=production webpack --progress --colors",
    "localPublish": "npm run build && npm publish",
    "clean": "rm build -rf"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ethcore/dapps-react-ui.git"
  },
  "keywords": [
    "dapps"
  ],
  "author": "ethcore team <admin@ethcore.io>",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/ethcore/dapps-react-ui/issues"
  },
  "homepage": "https://github.com/ethcore/dapps-react-ui#readme",
  "semistandard": {
    "parser": "babel-eslint"
  },
  "peerDependencies": {
    "react": "^15.0.0",
    "material-ui": "^0.15.0",
    "react-addons-css-transition-group": "^15.1.0",
    "react-tap-event-plugin": "^1.0.0",
    "react-tooltip": "^2.0.3"
  },
  "devDependencies": {
    "babel-core": "^6.5.2",
    "babel-eslint": "6.0.2",
    "babel-loader": "^6.2.3",
    "babel-plugin-transform-runtime": "^6.5.2",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "6.9.0",
    "babel-runtime": "^6.5.0",
    "chai": "3.5.0",
    "chai-enzyme": "0.4.2",
    "cheerio": "0.20.0",
    "css-loader": "^0.23.1",
    "enzyme": "2.3.0",
    "eslint": "^2.9.0",
    "eslint-config-semistandard": "^6.0.1",
    "eslint-config-standard": "^5.3.1",
    "eslint-config-standard-react": "^2.4.0",
    "eslint-plugin-promise": "^1.1.0",
    "eslint-plugin-react": "^5.1.1",
    "eslint-plugin-standard": "^1.3.2",
    "exports-loader": "0.6.3",
    "file-loader": "^0.8.5",
