    "webpack-error-notification": "0.1.6",
    "webpack-hot-middleware": "^2.7.1"
  },
  "dependencies": {
    "bignumber.js": "^2.3.0",
    "classnames": "^2.2.3",
    "ethereum-rpc-json": "*",
    "lodash.isequal": "^4.2.0",
    "napa": "^2.3.0"
  },
  "config": {
    "pre-git": {
      "pre-commit": [],
      "pre-push": [
        "npm run lint",
        "npm run testOnce"
      ],
      "post-commit": [],
      "post-checkout": [],
      "post-merge": []
    }
  }
