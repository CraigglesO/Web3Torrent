    "image-webpack-loader": "^1.8.0",
    "istanbul": "0.4.3",
    "jsdom": "9.2.1",
    "json-loader": "^0.5.4",
    "material-ui": "^0.15.0",
    "mocha": "^2.4.5",
    "mock-local-storage": "1.0.2",
    "napa": "2.3.0",
    "postcss-loader": "^0.8.1",
    "pre-git": "^3.8.1",
    "react": "^15.0.0",
    "react-addons-css-transition-group": "^15.1.0",
    "react-addons-test-utils": "15.1.0",
    "react-dom": "^15.0.0",
    "react-hot-loader": "^1.3.0",
    "react-redux": "^4.4.5",
    "react-router": "^2.4.1",
